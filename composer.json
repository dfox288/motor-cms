{
  "name": "motor-cms/motor-cms",
  "description": "Motor content management module",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "motor-cms/motor-backend": "0.10.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Motor\\CMS\\": "src",
      "Tests\\Motor\\CMS\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motor\\CMS\\Providers\\MotorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
